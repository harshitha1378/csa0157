#include <stdio.h>
int main() {
    int n, i;
    unsigned long fact = 1;
    printf("Enter an integer: ");
    scanf("%d", &n);
    if (n > 0) {
	
	 for (i = 1; i <= n; ++i) {
            fact *= i;
        }
    printf("product of first %d numbers(factorial) = %lu", n, fact);
     }
       
    else {
        printf("enter valid number");
    }

    return 0;
}
